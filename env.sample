# Environment Variables for Event Matcher App
# The actual file should be named .env.local

# MongoDB Atlas Connection String
MONGODB_URI=mongodb+srv://<username>:<password>@<cluster>.mongodb.net/<database>?retryWrites=true&w=majority

# OpenAI API Key
OPENAI_API_KEY=your_openai_api_key_here

# MongoDB Atlas Vector Search Index Names
EVENT_VECTOR_SEARCH_INDEX_NAME=event_vector_index
COURSE_VECTOR_SEARCH_INDEX_NAME=course_vector_index

